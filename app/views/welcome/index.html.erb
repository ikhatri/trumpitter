    <script>
        $('.play-popup-video').on('click', function (ev) {
            ev.preventDefault();
            $(".popup-video-overlay").addClass('on');
            $('body').addClass('overflow-hidden');
            $('html').addClass('overflow-hidden');
            setTimeout(function () {
                $('.popup-video-frame').attr('src', 'https://www.youtube.com/embed/DnpO_RTSNmQ?autoplay=1&rel=0&amp;fs=0&amp;showinfo=0');
            }, 300);
            $(".popup-video-overlay .close-popup-video").click(function (e) {
                e.preventDefault();
                $(".popup-video-overlay").addClass('exiting');
                setTimeout(function () {
                    $('.popup-video-frame').attr('src', 'https://www.youtube.com/embed/DnpO_RTSNmQ?');
                    $(".popup-video-overlay").removeClass('exiting').removeClass('on');
                }, 1000);
                $('body').removeClass('overflow-hidden');
                $('html').removeClass('overflow-hidden');
            });
        });
    </script>
