                            function sendTweet() {
                                //POST https://api.twitter.com/1.1/statuses/update.json?status=Maybe%20he%27ll%20finally%20find%20his%20keys.%20%23peterfalk
                                
                                <% 
                                def prepare_access_token(oauth_token, oauth_token_secret)
                                    consumer = OAuth::Consumer.new("APIKey", "APISecret", { :site => "https://api.twitter.com", :scheme => :header })

                                    # now create the access token object from passed values
                                    token_hash = { :oauth_token => oauth_token, :oauth_token_secret => oauth_token_secret }
                                    access_token = OAuth::AccessToken.from_hash(consumer, token_hash )

                                    return access_token
                                end

                                Twitter.configure do |config|
                                    config.consumer_key = 'YOUR_CONSUMER_KEY'
                                    config.consumer_secret = 'YOUR_CONSUMER_SECRET'
                                    config.oauth_token = 'YOUR_OAUTH_TOKEN'
                                    config.oauth_token_secret = 'YOUR_OAUTH_TOKEN_SECRET'
                                end

                                client = Twitter::Client.new
                                client.update('Hello, from Twitter Gem!')

                                %>
                                
                                $.ajax({ 
                                    url: "https://api.twitter.com/1.1/statuses/update.json",
                                    data: {
                                    status: document.getElementById("tweetFrame").innerHTML.document.getElementById("tweetTextOut").innerHTML
                                    },
                                    method: 'POST'
                                })
                            }
